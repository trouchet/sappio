{
  "name": "sappio",
  "type": "module",
  "version": "1.0.0",
  "description": "A minimal node app with logging.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-thread/sappio.git"
  },
  "keywords": [
    "nodejs",
    "app"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/web-thread/sappio/issues"
  }, 
  "homepage": "https://github.com/web-thread/sappio#readme",
  "scripts": {
    "docker:build-up": "sudo docker build -t sappio .&& sudo docker run -p 8080:8080 -d sappio",
    "docker:watch": "./utils/scripts/docker-watch.sh $1",
    "start": "nodemon ./src/index.js",
    "update": "bash ./utils/sscripts/update.sh",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --coverage",
    "test:ci": "jest --ci --reporters='default' --reporters='./github-actions-reporter'",
    "release": "np --any-branch",
    "nibble": "npx eslint-nibble .",
    "fixable-nibble": "npx eslint-nibble --fixable-only ."
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-nibble": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-scope": "7.1.1",
    "jest": "28.1.3",
    "lodash.combinations": "18.11.1",
    "np": "^3.0.4",
    "npm-check-engine": "1.0.0",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "helmet": "^6.0.0",
    "quivero-api": "^6.0.4"
  }
}
