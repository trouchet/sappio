{
  "name": "sappio",
  "description": "A minimal node app with logging.",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-thread/sappio.git"
  },
  "keywords": [
    "nodejs",
    "app"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/web-thread/sappio/issues"
  },
  "homepage": "https://github.com/web-thread/sappio#readme",
  "scripts": {
    "start": "nodemon ./index.js",
    "start:local": "npx knex migrate:latest && nodemon ./index.js",
    "start:prod": "npx knex migrate:latest && nodemon ./index.js",
    "build": "npm run node:build && npm run docker:build",
    "update": "bash ./scripts/update.sh",
    "knex": "node -r esm node_modules/.bin/knex",
    "knex:seeds": "npx knex seed:run",
    "node:build": "./scripts/node-build.sh",
    "docker:clean": "./scripts/docker-clean.sh $npm_config_tag",
    "docker:build": "./scripts/docker-build.sh",
    "docker:deploy": "./scripts/docker-deploy.sh $npm_config_tag $npm_config_from $npm_config_to",
    "docker:watch": "./scripts/docker-deploy.sh $npm_config_tag",
    "nibble": "npx eslint-nibble .",
    "fixable-nibble": "npx eslint-nibble --fixable-only .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "get-version": "echo $npm_package_version",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-scope": "7.1.1",
    "prettier": "^2.7.1",
    "swagger-autogen": "^2.22.0",
    "swagger-jsdoc": "^6.2.5"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@logtail/node": "^0.1.12",
    "@logtail/winston": "^0.1.13",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express-actuator": "^1.8.2",
    "express-json-validator-middleware": "^3.0.1",
    "express-jwt": "^7.7.7",
    "express-merror": "^1.0.0",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.6.0",
    "express-routes-catalogue": "^1.1.0",
    "express-sslify": "^1.2.0",
    "express-status-monitor": "^1.3.4",
    "express-validator": "^6.14.2",
    "express-youch": "^1.0.1",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jsrsasign": "^10.5.27",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.5.0"
  }
}
