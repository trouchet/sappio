{
  "name": "sappio",
  "description": "A minimal node app with logging.",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-thread/sappio.git"
  },
  "keywords": [
    "nodejs",
    "app"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/web-thread/sappio/issues"
  },
  "homepage": "https://github.com/web-thread/sappio#readme",
  "scripts": {
    "start": "nodemon ./index.js",
    "start:pm2": "pm2 start ./config/pm2_configs.js --attach --watch --time --wait-ready",
    "build": "npm run node:build && npm run docker:build",
    "node:build": "./scripts/node-build.sh",
    "docker:build": "./scripts/docker-build.sh",
    "test": "jest --collect-coverage --coverage",
    "update": "bash ./scripts/update.sh",
    "docker:clean": "./scripts/docker-clean.sh $npm_config_tag",
    "docker:deploy": "./scripts/docker-deploy.sh $npm_config_tag $npm_config_from $npm_config_to",
    "docker:watch": "./scripts/docker-deploy.sh $npm_config_tag",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "nibble": "npx eslint-nibble .",
    "fixable-nibble": "npx eslint-nibble --fixable-only .",
    "get-version": "echo $npm_package_version",
    "deploy": "pm2-runtime start ecosystem.config.js --env production"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-scope": "7.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.7.1",
    "superagent": "^8.0.3",
    "supertest": "^6.3.1",
    "swagger-autogen": "^2.22.0",
    "swagger-jsdoc": "^6.2.5",
    "ts-jest": "^29.0.3"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.0.0",
    "@logtail/node": "^0.1.12",
    "@logtail/winston": "^0.1.13",
    "axios": "^1.1.3",
    "compression": "^1.7.4",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express-actuator": "^1.8.2",
    "express-json-validator-middleware": "^3.0.1",
    "express-jwt": "^7.7.7",
    "express-merror": "^1.0.0",
    "express-rate-limit": "^6.6.0",
    "express-route-parser": "^1.0.5",
    "express-sslify": "^1.2.0",
    "express-validator": "^6.14.2",
    "express-youch": "^1.0.1",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "jsrsasign": "^10.5.27",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "prom-client": "13.2.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "swagger-stats": "^0.99.2",
    "swagger-ui-express": "^4.5.0",
    "tx2": "^1.0.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
